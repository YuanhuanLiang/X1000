config SOC_CAMERA_DOUBLE_CHANNEL
	bool "soc camera double channel"
	depends on SOC_CAMERA_GC0328_DOUBLE || SOC_CAMERA_GC0308_DOUBLE
	default y
	help
	  This is a V4L2 camera driver for the sensor double channel

